{
  "name": "quiz-helper",
  "version": "1.0.0",
  "description": "A TypeScript tool to help with answering quiz questions using AI (ethical proof of concept)",
  "main": "dist/index.js",
  "bin": {
    "quiz-helper": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "start": "tsx src/index.ts",
    "dev": "tsx watch src/index.ts",
    "test": "tsx test/config-validation.ts",
    "demo": "npx serve -p 8000"
  },
  "keywords": [
    "quiz",
    "automation",
    "ai",
    "playwright"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.10.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@ai-sdk/openai": "3.0.0-beta.60",
    "ai": "6.0.0-beta.99",
    "dotenv": "17.2.3",
    "playwright": "1.56.1",
    "playwright-core": "1.56.1",
    "zod": "4.1.12"
  }
}